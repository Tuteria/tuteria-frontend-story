{
  "name": "test_app",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "build:lib": "npx rollup -c",
    "fix:js": "npx eslint --fix ./src/",
    "lint:js": "npx eslint ./src/",
    "lint:style": "npx stylelint ./src/",
    "fix:style": "npx stylelint ./src/ --fix",
    "start:docs": "docz dev",
    "build:docs": "docz build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "style": "lib/default.css",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "CI=true yarn test --passWithNoTests"
    }
  },
  "devDependencies": {
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "cra-template-npm-library": "1.0.8",
    "react-scripts": "3.3.0",
    "rollup": "2.42.2",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-includepaths": "0.2.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-stylelint": "1.0.0",
    "rollup-plugin-terser": "7.0.2",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "21.0.0",
    "stylelint-order": "4.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  }
}
