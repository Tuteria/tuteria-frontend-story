{
  "name": "tuteria-mobile-codebase",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public --ci",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install",
    "clean": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@capacitor/browser": "^1.0.2",
    "@capacitor/cli": "3.1.1",
    "@chakra-ui/react": "1.6.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@ionic/react": "^5.5.0",
    "@ionic/react-router": "^5.5.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "framer-motion": "^4",
    "ionicons": "^5.4.0",
    "lodash.isempty": "^4.4.0",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/pluralize": "^0.0.29",
    "@types/react-datepicker": "^4.1.3",
    "@types/react-select": "^4.0.17",
    "husky": "^7.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "description": "An Ionic project",
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  }
}
