{
  "name": "tuteria-mobile-codebase",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public --ci",
    "build-storybook": "build-storybook -s public",
    "build:windows": "cd ./storybook-static && now --no-clipboard --token=%NOW_TOKEN% --prod",
    "deploy:linux": "cd ./storybook-static && vercel --no-clipboard --token=$NOW_TOKEN --prod",
    "prepare": "husky install",
    "clean": "prettier --write",
    "test:single": "node --inspect -r tsm tests/demo.ts",
    "test:can_proceed": "node --inspect -r tsm tests/can-proceed.ts",
    "shared-lib:lint": "npx nx run shared-lib:lint",
    "shared-lib:test": "npx nx run shared-lib:test",
    "shared-lib:storybook": "npx nx run shared-lib:storybook",
    "design-system:storybook": "npx nx run design-system:storybook"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@capacitor/browser": "^1.0.2",
    "@capacitor/cli": "3.1.1",
    "@capacitor/core": "^3.1.2",
    "@capacitor/storage": "^1.0.3",
    "@chakra-ui/icons": "1.0.15",
    "@chakra-ui/react": "1.6.10",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@ionic/react": "^5.5.0",
    "@ionic/react-router": "^5.5.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/node-logger": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^12.6.3",
    "@types/node": "^12.19.15",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "core-js": "^3.6.5",
    "framer-motion": "^4",
    "ionicons": "^5.4.0",
    "katex": "^0.13.18",
    "lodash.isempty": "^4.4.0",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0",
    "use-mobile-detect-hook": "^1.0.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/cli": "latest",
    "@nrwl/eslint-plugin-nx": "13.9.5",
    "@nrwl/jest": "13.9.5",
    "@nrwl/linter": "13.9.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "^13.9.5",
    "@nrwl/storybook": "^13.9.5",
    "@nrwl/web": "13.9.5",
    "@nrwl/workspace": "latest",
    "@storybook/builder-webpack5": "~6.4.12",
    "@storybook/manager-webpack5": "~6.4.12",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/pluralize": "^0.0.29",
    "@types/react": "17.0.40",
    "@types/react-datepicker": "^4.1.3",
    "@types/react-dom": "17.0.13",
    "@types/react-select": "^4.0.17",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "babel-jest": "27.2.3",
    "babel-loader": "8.1.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.1",
    "jest": "27.2.3",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "nx": "^13.9.5",
    "prettier": "^2.3.2",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.5",
    "tsm": "^2.2.1",
    "typescript": "^4.3.5",
    "url-loader": "^3.0.0",
    "uvu": "^0.5.3"
  },
  "description": "An Ionic project",
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  }
}
